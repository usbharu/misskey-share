{
  "name": "misskey-share",
  "manifest_version": 3,
  "version": "1.0.0",
  "content_scripts": [{
    "matches": ["https://*/*","http://*/*"],
    "js": ["content.js"]
  }],
  "web_accessible_resources": [{
    "matches": ["https://*/*","http://*/*"],
    "resources": ["inject.js"]
  }],
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  }
}